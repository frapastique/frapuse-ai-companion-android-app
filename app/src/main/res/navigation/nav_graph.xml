<?xml version="1.0" encoding="utf-8"?>
<navigation
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/nav_graph"
        app:startDestination="@id/homeFragment">

    <fragment
            android:id="@+id/textToImageFragment"
            android:name="com.back.frapuse.ui.imagegen.TextToImageFragment"
            android:label="TextToImageFragment"
            tools:layout="@layout/fragment_text_to_image" >
        <action
                android:id="@+id/action_textToImageFragment_to_homeFragment"
                app:destination="@id/homeFragment" />
    </fragment>
    <fragment
            android:id="@+id/homeFragment"
            android:name="com.back.frapuse.ui.HomeFragment"
            android:label="HomeFragment"
            tools:layout="@layout/fragment_home">
        <action
                android:id="@+id/action_homeFragment_to_textToImageFragment"
                app:destination="@id/textToImageFragment" />
    </fragment>
</navigation>